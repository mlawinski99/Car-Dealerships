@{
    ViewData["Title"] = "Panel Administratora";
    Layout = "~/Views/Admin/Dealerships/_Layout.cshtml";
    ViewData["ActivePage"] = "Dealerships/DealershipList";
}
<ul class="list-group list-group-flush">
    @foreach (Dealership dealership in @ViewBag.dealershipList)
    {
        <li class="list-group-item">
            <div class="d-flex w-100">
                <h5 class="mb-1">@dealership.DealershipName</h5>
            </div>
            <p class="mb-1">@dealership.Address.AddressStreet @dealership.Address.AddressApartmentNumber</p>
            <p class="mb-1">@dealership.Address.AddressPostalCode @dealership.Address.AddressCity</p>
            <small>reszta info...</small>
        </li>
    }
</ul>