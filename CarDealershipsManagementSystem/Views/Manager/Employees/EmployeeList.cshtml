@{
    ViewData["Title"] = "Panel Managera";
    Layout = "~/Views/Manager/Employees/_Layout.cshtml";
    ViewData["ActivePage"] = "Employees/EmployeeList";
}
<ul class="list-group list-group-flush">
    @foreach (Employee employee in @ViewBag.employeeList)
    {
        <li class="list-group-item">
            <div class="d-flex w-100">
                <h5 class="mb-1">@employee.ApplicationUser.FirstName @employee.ApplicationUser.LastName</h5>
            </div>
            <p class="mb-1">@employee.ApplicationUser.Email</p>
            <p class="mb-1">@employee.ApplicationUser.PhoneNumber</p>
            <small>reszta info...</small>
        </li>
    }
</ul>